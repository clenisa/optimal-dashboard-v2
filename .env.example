# Development Configuration
NODE_ENV=development

# Required Configuration
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Stripe (server-side)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Stripe (client-side)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=

# Optional
NEXT_PUBLIC_ELECTRON_CONSOLE_URL=http://localhost:3000
# Optimal Desktop Enhanced Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration (your existing setup)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Stripe Configuration (for credit purchases)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# ElectronConsole Configuration (your local setup)
# Default port for ElectronConsole - change if you use a different port
NEXT_PUBLIC_ELECTRON_CONSOLE_URL=http://localhost:3000

# Optional: Analytics and Monitoring
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id

